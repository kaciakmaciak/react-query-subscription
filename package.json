{
  "name": "react-query-subscription",
  "version": "1.8.1",
  "engines": {
    "node": ">=10.13.0"
  },
  "description": "Hook for managing, caching and syncing observables in React",
  "author": {
    "name": "Katarina Anton"
  },
  "keywords": [
    "react",
    "react-query",
    "real-time",
    "subscription"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kaciakmaciak/react-query-subscription"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/react-query-subscription.umd.js",
  "types": "types/index.d.ts",
  "sideEffects": [],
  "files": [
    "dist",
    "lib",
    "es",
    "types"
  ],
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "prerelease": "npm run build",
    "release": "semantic-release",
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "rimraf ./lib && cross-env BABEL_ENV=commonjs babel --extensions .ts,.tsx --ignore ./src/**/tests/**/* ./src --out-dir lib",
    "build:esm": "rimraf ./es && babel --extensions .ts,.tsx --ignore ./src/**/tests/**/* ./src --out-dir es",
    "build:umd": "rimraf ./dist && cross-env NODE_ENV=production rollup -c && rollup-plugin-visualizer stats.json",
    "build:types": "rimraf ./types && tsc --project ./tsconfig.types.json",
    "docs:pages": "typedoc --readme ./README.md",
    "stats": "open ./stats.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "lint-staged": {
    "src/**/!(__tests__)/*.{js,ts,jsx,tsx}": [
      "pretty-quick --staged",
      "npm run lint -- --fix"
    ],
    "*.(md|html|json)": [
      "pretty-quick --staged"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-query": "^3.0.0",
    "rxjs": "^7.1.0"
  },
  "dependencies": {
    "@babel/runtime": "7.14.6"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.14.6",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-replace": "2.4.2",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/git": "10.0.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.1",
    "@types/jest": "26.0.23",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "babel-jest": "27.0.6",
    "conventional-changelog-conventionalcommits": "4.6.1",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.0.1",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest-dom": "3.9.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.6.0",
    "eventsource": "2.0.2",
    "husky": "7.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "msw": "0.35.0",
    "prettier": "2.3.2",
    "pretty-quick": "3.1.1",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.3",
    "react-query": "3.39.2",
    "rimraf": "3.0.2",
    "rollup": "2.52.6",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-sizes": "1.0.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.5.2",
    "rxjs": "7.1.0",
    "semantic-release": "19.0.3",
    "typedoc": "0.22.15",
    "typescript": "4.3.5"
  }
}
